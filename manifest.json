{
  "manifest_version": 3,
  "name": "Text Extractor Extension",
  "version": "1.0",
  "description": "Extracts and processes text from the screen.",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Text Extractor"
  }
}
